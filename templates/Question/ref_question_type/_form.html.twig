
{{ form_start(form) }}
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label>{{ "Name"|trans }}</label>
            {{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}
            <small id="" class="text-danger">
                {{ form_errors(form.name) }}
            </small>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label>{{ "Enabled"|trans }}</label>
            {{ form_widget(form.enabled) }}
            <small id="" class="text-danger">
                {{ form_errors(form.enabled) }}
            </small>
        </div>
    </div>
    {{ form_rest(form) }}
    <div class="col-md-12">
        <a href="{{path('admin_category')}}" class="btn btn-info float-left" type="button">{{ "Back to the list"|trans }} </a>
        <button class="btn btn-primary float-right" type="submit">{{ "Save"|trans }}</button>
    </div>
{{ form_end(form) }}